{
    "builds": [
        {
            "src": "/api/**",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "/vendor/**",
            "use": "@vercel/static"
        },
        {
            "src": "/images/**",
            "use": "@vercel/static"
        }
    ],
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(vendor|images)/(.*)",
            "dest": "$1/$2"
        },
        {
            "src": "/dashboard-maps.php",
            "dest": "/api/maps.php"
        },
        {
            "src": "/dashboard-mostactive.php",
            "dest": "/api/mostactive.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}