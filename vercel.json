{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        },
        "vendor/**": {
            "runtime": "@vercel/static"
        },
        "images/**": {
            "runtime": "@vercel/static"
        }
    },
    "routes": [
        {
            "src": "/(vendor|images)/(.*)",
            "dest": "$1/$2"
        },
        {
            "src": "/dashboard-maps.php",
            "dest": "/api/maps.php"
        },
        {
            "src": "/dashboard-mostactive.php",
            "dest": "/api/mostactive.php"
        },
        {
            "src": "/dashboard-player.php",
            "dest": "/api/player.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}